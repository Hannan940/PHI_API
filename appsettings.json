{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PHIConfig": {
    "ValidFormats": [ ".txt" ],
    "RequestsDirectoryPath": "wwwroot/PHIRequests",
    "SanitizedDirectoryPath": "wwwroot/PHIProcessed"
  }
}
